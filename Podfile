platform :ios, '13.0'

source 'https://mirrors.tuna.tsinghua.edu.cn/git/CocoaPods/Specs.git'
use_frameworks!
inhibit_all_warnings!
install! 'cocoapods',
         :deterministic_uuids => false #去除警告


target 'FeedbackProject' do

  pod 'Alamofire' #网络请求
  pod 'SnapKit' #约束
  pod 'SVProgressHUD' #提示框
  pod 'IQKeyboardManagerSwift' #键盘管理
  pod 'RealmSwift' #数据库存储
  pod 'Charts' #图表
  pod 'PGDatePicker' #时间选择器
  pod 'WMPageController' #分段选择
  
end
